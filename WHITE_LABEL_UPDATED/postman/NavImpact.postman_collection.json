{
  "info": {
    "name": "NavImpact API",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
    "description": "Key endpoints for NavImpact API"
  },
  "item": [
    {"name": "Health", "request": {"method": "GET", "url": "{{API_URL}}/health"}},
    {"name": "Tasks - Create", "request": {"method": "POST", "header": [{"key": "Content-Type", "value": "application/json"},{"key": "Authorization", "value": "Bearer {{TOKEN}}"}], "url": "{{API_URL}}/api/v1/tasks/", "body": {"mode": "raw", "raw": "{\n  \"title\": \"New Task\",\n  \"description\": \"Details\",\n  \"assignee_id\": 1\n}"}}},
    {"name": "ML Insights", "request": {"method": "GET", "header": [{"key": "Authorization", "value": "Bearer {{TOKEN}}"}], "url": "{{API_URL}}/api/v1/ml-analytics/insights"}}
  ],
  "variable": [
    {"key": "API_URL", "value": "https://navimpact-api.onrender.com"},
    {"key": "TOKEN", "value": ""}
  ]
} 